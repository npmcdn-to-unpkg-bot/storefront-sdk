{
  "url": "http://api.beta.vtex.com/:account/products/",
  "cacheDuration": 60,
  "parameters": {
    "account": "{{ account.name }}"
  },
  "queryString": {
    "query": "{{ parameters.query }}",
    "category": "{{ parameters.category }}",
    "brands": "{{ parameters.brands }}",
    "page": "{{ parameters.page }}",
    "pageSize": "{{ parameters.pageSize }}"
  }
}
